{
  "name": "mimmi20/laminas-form-element-links",
  "description": "Provides an additional Form Element for laminas-form to render links inside a form",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-links",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-links/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-links"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-form": "^3.21.0",
    "laminas/laminas-inputfilter": "^2.33.0",
    "laminas/laminas-view": "^2.41.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.31.2",
    "laminas/laminas-i18n": "^2.30.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-navigation": "^2.20.0",
    "laminas/laminas-servicemanager": "^3.23.0",
    "mimmi20/coding-standard": "^6.1.13",
    "nikic/php-parser": "^5.6.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.22",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.3.8",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^2.1.6",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.3.3",
    "symplify/phpstan-rules": "^14.7.0",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-navigation": "required to use objects of type AbstractPage to render the link",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Links\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Links\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Links",
      "config-provider": "Mimmi20\\Form\\Links\\ConfigProvider"
    }
  }
}
