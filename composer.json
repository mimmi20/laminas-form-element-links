{
  "name": "mimmi20/laminas-form-element-links",
  "description": "Provides an additional Form Element",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-links",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-links/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-links"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "laminas/laminas-form": "^3.10.1",
    "laminas/laminas-hydrator": "^4.13.0",
    "laminas/laminas-inputfilter": "^2.24.0",
    "laminas/laminas-validator": "^2.30.1",
    "laminas/laminas-view": "^2.27.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.26.19",
    "laminas/laminas-i18n": "^2.22.0",
    "laminas/laminas-modulemanager": "^2.14.0",
    "laminas/laminas-navigation": "^2.18.1",
    "mimmi20/coding-standard": "4.0.2",
    "nikic/php-parser": "^v4.15.4",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.10.8",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-phpunit": "^1.3.10",
    "phpunit/phpunit": "^10.0.18"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-navigation": "required to use objects of type AbstractPage to render the link",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Links\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Links\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Links",
      "config-provider": "Mimmi20\\Form\\Links\\ConfigProvider"
    }
  }
}
